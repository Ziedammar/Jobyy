{% extends 'base.html.twig' %}
     {% block stylesheet  %}
     {% endblock %}
      {% block nav %}

		  {% include 'Inc/nav2.html.twig' %}

	  {% endblock %}


{% block body %}			<div class="clearfix"></div>
			
			<!-- Title Header Start -->
			<section class="inner-header-title" style="background-image:url(assets/img/banner-10.jpg);">
				<div class="container">
					<h1>Browse Candidat</h1>
				</div>
			</section>
			<div class="clearfix"></div>
			<!-- Title Header End -->
			
			
			
			<!-- Browse Company List Start -->
			<section class="browse-company">
				<div class="container js-filter">
				
					<!-- Company Searrch Filter Start -->
					<div class="row extra-mrg">
						<div class="wrap-search-filter">
							<form>
					
							  <div class="col-4 col-sm-2">
															{{form_start(form)}}
															{{form_row(form.c)}}
															{{form_row(form.min)}}
															{{form_row(form.max)}}

															<button type="submit" class="btn btn-success"> Filtrer </button>
															{{form_end(form)}}


								</div>
					
							   <div class="col-md-2 col-sm-2">
									<a href="{{path ('interview_enteprise_calender')}}"<button type="submit" class="btn btn-success full-width">Calendrier</button></a>
								</div>
								<div class="col-md-2 col-sm-2">
									<a class =" btn btn-info js-filter-button1" href="{{ path ('interview_enteprise_date')}}"<button type="submit" class="btn btn-success full-width">Sort </button></a>
								</div>
							</form>
						</div>
					</div>
					<!-- Company Searrch Filter End -->
					
					<!-- Single Browse Company -->
                    {% for interview in interviews %}
					<div class="item-click js-filter-article ">
						<article>
							<div class="brows-company">
								<div class="col-md-2 col-sm-2">
									<div class="brows-company-pic">
										<img src="{{ asset('uploads/'~ interview.getCand().getCandidateId().getProfilePic() ) }}" class="img-responsive" alt="" />
							</div>
								</div>
								<div class="col-md-4 col-sm-4">
									<div class="brows-company-name">
										<a href="/company-detail"><h4>{{interview.getCand().getCandidateId().getEmail()}}</h4></a>
										<span class="brows-company-tagline">{{interview.abreviation}}</span>

									</div>
								</div>
								<div class="col-md-2 col-sm-2">
									<div class="brows-company-location">
										<p>{{interview.getDateTemps()|date('Y-m-d')}}</p>
									</div>
								</div>
                               
                                
								<div class="col-md-4 col-sm-4">
									<div class="brows-company-position">
                                	<a href="{{ path('editer_interview', {'id': interview.id}) }}" class="btn advance-search" title="apply">Editer</a>
                                    
                                     <form method="post" action="{{ path('delete_interview', {'id': interview.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
                                            <input type="hidden" name="_method" value="DELETE">
                                            <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ interview.id) }}">
                                            <button class="btn">Delete</button>
                                        </form>

									</div>
								</div>
							</div>
						</article>
					</div>
					{% endfor %}
					<!-- Single Browse Company -->
	
				
				
					
				
				
				
					
					<div class="row">
						<ul class="pagination">
							<li><a href="#">&laquo;</a></li>
							<li class="active"><a href="#">1</a></li>
							<li><a href="#">2</a></li>
							<li><a href="#">3</a></li> 
							<li><a href="#">4</a></li> 
							<li><a href="#"><i class="fa fa-ellipsis-h"></i></a></li> 
							<li><a href="#">&raquo;</a></li> 
						</ul>
					</div>
					
				</div>
			</section>
			<!-- Browse Company List End -->
			

			<!-- Sign Up Window Code -->
			<div class="modal fade" id="signup" tabindex="-1" role="dialog" aria-labelledby="myModalLabel2" aria-hidden="true">
				<div class="modal-dialog">
					<div class="modal-content">
						<div class="modal-body">
							<div class="tab" role="tabpanel">
							<!-- Nav tabs -->
							<ul class="nav nav-tabs" role="tablist">
								<li role="presentation" class="active"><a href="#login" role="tab" data-toggle="tab">Sign In</a></li>
								<li role="presentation"><a href="#register" role="tab" data-toggle="tab">Sign Up</a></li>
							</ul>
							<!-- Tab panes -->
							<div class="tab-content" id="myModalLabel2">
								<div role="tabpanel" class="tab-pane fade in active" id="login">
									<img src="assets/img/logo.png" class="img-responsive" alt="" />
									<div class="subscribe wow fadeInUp">
										<form class="form-inline" method="post">
											<div class="col-sm-12">
												<div class="form-group">
													<input type="email"  name="email" class="form-control" placeholder="Username" required="">
													<input type="password" name="password" class="form-control"  placeholder="Password" required="">
													<div class="center">
													<button type="submit" id="login-btn" class="submit-btn"> Login </button>
													</div>
												</div>
											</div>
										</form>
									</div>
								</div>

								<div role="tabpanel" class="tab-pane fade" id="register">
								<img src="assets/img/logo.png" class="img-responsive" alt="" />
									<form class="form-inline" method="post">
											<div class="col-sm-12">
												<div class="form-group">
													<input type="text"  name="email" class="form-control" placeholder="Your Name" required="">
													<input type="email"  name="email" class="form-control" placeholder="Your Email" required="">
													<input type="email"  name="email" class="form-control" placeholder="Your Email" required="">
													<input type="email"  name="email" class="form-control" placeholder="Username" required="">
													<input type="password" name="password" class="form-control"  placeholder="Password" required="">
													<div class="center">
													<button type="submit" id="subscribe" class="submit-btn"> Create Account </button>
													</div>
												</div>
											</div>
										</form>
								</div>
							</div>
							</div>
						</div>
						</div>
				</div>
			</div>   
			<!-- End Sign Up Window -->

{% endblock %}
{% block javascript  %}
    <script type="module" src="{{ asset("../../app.js") }}"></script>

{% endblock %}
